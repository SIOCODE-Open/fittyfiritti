{
  "name": "@fittyfiritti/built-in-ai-api",
  "version": "0.1.0",
  "type": "module",
  "description": "Type definitions for the Chrome Built-in AI API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "*",
    "eslint": "*",
    "prettier": "*",
    "prettier-plugin-organize-imports": "*"
  },
  "publishConfig": {
    "access": "restricted"
  }
}