{
  "name": "@diai/editor",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "react": "*",
    "react-dom": "*",
    "@ricky0123/vad-react": "*",
    "@diai/built-in-ai-api": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "@vitejs/plugin-react": "*",
    "vite": "*",
    "typescript": "*",
    "eslint": "*",
    "prettier": "*",
    "prettier-plugin-organize-imports": "*",
    "tailwindcss": "*",
    "@tailwindcss/vite": "*"
  }
}